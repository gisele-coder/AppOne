<resources>
    <!-- Base application theme. -->
    <style name="Theme.AppOne" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Personalize os atributos conforme necessÃ¡rio -->
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_primary</item>
        <item name="colorPrimaryVariant">@color/dark_secundary</item>
        <item name="colorOnPrimary">@color/dark_text</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dark_text_highlight</item>
        <item name="colorSecondaryVariant">@color/white</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.AppOne.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AppOne.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.AppOne.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>